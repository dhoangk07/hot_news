<div class="container-fluid col-lg-8">
  <div >
    <%= form_for(@rsslink, html: {class: "form-horizontal", role: "form"}) do |f| %>
    <div class='form-group'>
      <div class='col-sm-8'>
        <%= f.text_field :url, class:'form-control', placeholder: "Enter url" %>
      </div>
    </div>
    <%= f.submit "Create", class: 'btn btn-primary btn-lg' %>
    <% end %>
  </div>

  <div >
  <%= link_to 'Back', events_path%>
</div>
</div>




